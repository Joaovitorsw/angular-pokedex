{
  "name": "angular-pokedex",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./setenv.ts",
    "start": "npm run config -- --environment=dev && ng s -o",
    "build": "npm run config -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "barrel": "barrelsby -e .spec.ts --delete -l all -d ./src/app",
    "test": "ng test",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "^13.2.2",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/material": "^13.2.2",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@angular/service-worker": "~13.2.0",
    "@ngneat/until-destroy": "^9.0.0",
    "@schematics/angular": "^13.2.3",
    "barrelsby": "^2.3.2",
    "dotenv": "^16.0.0",
    "ng-particles": "^2.40.2",
    "rxjs": "~7.5.0",
    "ts-mockito": "^2.6.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.2.3",
    "@angular/cli": "~13.2.3",
    "@angular/compiler-cli": "~13.2.0",
    "@cypress/schematic": "^1.6.0",
    "@nrwl/angular": "^13.8.1",
    "@nrwl/cli": "^13.8.1",
    "@testing-library/angular": "^11.0.2",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "cypress": "latest",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
